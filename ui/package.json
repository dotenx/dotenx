		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"fmt": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --check ./src && eslint ./src --ext .js,.jsx,.ts,.tsx && tsc"
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@emotion/server": "^11.10.0",
