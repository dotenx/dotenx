	"name": "ao-ui",
	"version": "0.1.0",
	"dependencies": {
		"@codemirror/legacy-modes": "^0.19.0",
		"@codemirror/stream-parser": "^0.19.9",
		"@hookform/error-message": "^2.0.0",
		"@hookform/resolvers": "^2.8.8",
		"@mantine/core": "^4.2.12",
		"@mantine/hooks": "^4.2.12",
		"@types/use-persisted-state": "^0.3.1",
		"@uiball/loaders": "^1.2.6",
		"@uiw/react-codemirror": "4.3.3",
		"@uiw/react-textarea-code-editor": "^2.0.2",
		"axios": "^0.26.1",
		"clsx": "^1.1.1",
		"color": "^4.2.3",
		"dagre": "^0.8.5",
		"date-fns": "^2.28.0",
		"framer-motion": "^6.3.1",
		"fuse.js": "^6.5.3",
		"jotai": "^1.6.1",
		"js-cookie": "^3.0.1",
		"lodash": "^4.17.21",
		"nanoid": "^3.3.3",
		"prism-react-renderer": "^1.3.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-flow-renderer": "^9.7.4",
		"react-hook-form": "^7.28.0",
		"react-hotkeys-hook": "^3.4.4",
		"react-icons": "^4.3.1",
		"react-modal": "^3.14.4",
		"react-query": "^3.34.16",
		"react-router-dom": "^6.2.2",
		"react-scripts": "5.0.0",
		"react-select": "^5.2.2",
		"react-table": "^7.7.0",
		"react-toastify": "^8.2.0",
		"react-use-clipboard": "^1.0.8",
		"use-persisted-state": "^0.3.3",
		"web-vitals": "^2.1.4",
		"zod": "^3.13.4"
	},
	"scripts": {
		"dev": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore '**/*.{ts,tsx}' && tsc --noEmit"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/recommended",
			"plugin:@typescript-eslint/eslint-recommended",
			"plugin:react/recommended",
			"plugin:react/jsx-runtime"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			},
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"plugins": [
			"react",
			"@typescript-eslint",
			"react-hooks"
		],
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"rules": {
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/color": "^3.0.3",
		"@types/dagre": "^0.7.47",
		"@types/js-cookie": "^3.0.2",
		"@types/lodash": "^4.14.179",
		"@types/node": "^18.0.1",
		"@types/react": "^17.0.40",
		"@types/react-dom": "^17.0.13",
		"@types/react-modal": "^3.13.1",
		"@types/react-table": "^7.7.11",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"autoprefixer": "^10.4.4",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-react": "^7.29.3",
		"eslint-plugin-react-hooks": "^4.3.0",
		"postcss": "^8.4.12",
		"prettier": "^2.5.1",
		"tailwind-scrollbar": "^1.3.1",
		"tailwindcss": "^3.0.23",
		"typescript": "^4.6.2"
	}
}
