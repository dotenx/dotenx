		"@mantine/core": "^5.8.4",
		"@mantine/form": "^5.8.4",
		"@mantine/hooks": "^5.8.4",
		"@mantine/modals": "^5.8.4",
		"@mantine/notifications": "^5.8.4",
		"@mantine/prism": "^5.8.4",
		"@monaco-editor/react": "^4.4.6",
		"@tabler/icons": "^1.114.0",
		"@types/use-persisted-state": "^0.3.1",
		"@uiball/loaders": "^1.2.6",
		"@uiw/react-textarea-code-editor": "^2.0.6",
		"axios": "^1.2.0",
		"clsx": "^1.2.1",
		"color": "^4.2.3",
